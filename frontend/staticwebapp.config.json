{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*"]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html"
  }
} 